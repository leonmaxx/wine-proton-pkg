+    .get_fd = dri2_get_fd,
+    .window_buffer_from_dmabuf = dri2_window_buffer_from_dmabuf,
+    .copy_front = dri2_copy_front,
+    .present_pixmap = dri2_present_pixmap,
+    .destroy_pixmap = dri2_destroy_pixmap,
+};
