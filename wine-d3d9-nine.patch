+    .deinit = dri3_deinit,
+    .get_fd = dri3_get_fd,
+    .window_buffer_from_dmabuf = dri3_window_buffer_from_dmabuf,
+    .copy_front = dri3_copy_front,
+    .present_pixmap = dri3_present_pixmap,
+    .destroy_pixmap = dri3_destroy_pixmap,
+};
